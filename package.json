{
  "name": "simple-regex",
  "displayName": "Simple Regex",
  "description": "Highlights regex patterns in Python, Java, Golang and Rust files.",
  "version": "0.0.1",
  "engines": {
    "vscode": "^1.89.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [
    "onLanguage:java",
    "onLanguage:python",
    "onLanguage:golang",
    "onLanguage:rust"
  ],
  "main": "./extension.js",
  "contributes": {
    "commands": [
      {
        "command": "simple-regex.helloWorld",
        "title": "Hello World"
      }
    ]
  },
  "scripts": {
    "lint": "eslint .",
    "pretest": "npm run lint",
    "test": "vscode-test"
  },
  "devDependencies": {
    "@types/mocha": "^10.0.6",
    "@types/node": "18.x",
    "@types/vscode": "^1.89.0",
    "@vscode/test-cli": "^0.0.8",
    "@vscode/test-electron": "^2.3.9",
    "eslint": "^8.57.0",
    "typescript": "^5.4.5"
  },
  "dependencies": {
    "regex-parser": "^2.3.0"
  }
}
